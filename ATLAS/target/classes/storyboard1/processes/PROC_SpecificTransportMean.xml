<?xml version="1.0" encoding="UTF-8"?>
<tns:process name="PROC_SpecificTransportMean" xmlns:tns="http://das.fbk.eu/Process"
	xmlns:tns1="http://das.fbk.eu/Annotation" xmlns:tns2="http://das.fbk.eu/DataType"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://das.fbk.eu/Process ../../../schema/allow-ensembles/Process.xsd ">

	<tns:receive name="STM_JourneyRequest">
		<tns:precondition>
			<tns1:point>
				<tns1:domainProperty dp_name="SpecificJourney">
					<tns1:state>INITIAL</tns1:state>
				</tns1:domainProperty>
			</tns1:point>
		</tns:precondition>
		<tns:actionVariable name="SelectedAlternative" type="string">
			<tns2:content type="anyType"></tns2:content>
		</tns:actionVariable>
		<tns:actionVariable name="CurrentSegment" type="string">
			<tns2:content type="anyType"></tns2:content>
		</tns:actionVariable>
	</tns:receive>

	<tns:concrete name="STM_ServiceCall">
		<tns:effect>
			<tns1:event dpName="SpecificJourney" eventName="processRequest"></tns1:event>
		</tns:effect>
	</tns:concrete>
	

	<tns:invoke name="STM_JourneyResponse">
		<tns:precondition>
			<tns1:point>
				<tns1:domainProperty dp_name="SpecificJourney">
					<tns1:state>JOURNEY_REQUESTED</tns1:state>
				</tns1:domainProperty>
			</tns1:point>
		</tns:precondition>
		<tns:effect>
			<tns1:event dpName="SpecificJourney" eventName="sendRoutes"></tns1:event>
		</tns:effect>
		<tns:actionVariable name="ResultList" type="string">
			<tns2:content type="anyType"></tns2:content>
		</tns:actionVariable>
	</tns:invoke>
</tns:process>
